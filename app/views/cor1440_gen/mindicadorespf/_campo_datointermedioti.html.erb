<%= f.input :id, as: :hidden %>
<div class="panel panel-default">
  <div class="panel-heading"><%= t('Datos intermedios') %></div>
  <div class="panel-body">

    <div class="div-campostind">
      <table class='table table-bordered' border=1 width="100%">
        <thead>
          <tr>
            <th><%= t('Nombre') %></th>
            <th><%= t('Nombre interno') %></th>
            <th><%= t('Función de medición') %></th>
            <th><%= t('Acciones') %></th>
          </tr>
        </thead>
        <tbody id="datointermedioti">
          <%= f.simple_fields_for :datointermedioti, 
            f.object.datointermedioti.order(:id) do |o| %>
            <%= render 'cor1440_gen/mindicadorespf/datointermedioti_campos', f:  o %>
          <% end %>
        </tbody>
      </table>
      <div class="links">
        <%= link_to_add_association t('Añadir dato intermedio'), f, 
          :datointermedioti, {
          :"data-association-insertion-node" => "tbody#datointermedioti", 
          :"data-association-insertion-method" => "append", 
          partial: 'cor1440_gen/mindicadorespf/datointermedioti_campos',
          class: 'btn btn-sm btn-primary' } %>
      </div> <!-- links -->
    </div> <!-- .div-campostind -->
  </div><!-- panel-body -->
</div><!-- panel -->
