<p></p>
<div class="panel panel-default">
  <div class="panel-heading"><%= t('Resultados') %></div>
  <div class="panel-body">
    <div class="div-resultadospf">
      <table class='table table-bordered' border=1 width="100%">
        <thead>
          <tr>
            <th><%= t('refobjetivo') %></th>
            <th width="10%"><%= t('Código') %></th>
            <th><%= t('Resultado') %></th>
            <th><%= t('Acciones') %></th>
          </tr>
        </thead>
        <tbody id="resultadospf">
          <%= f.simple_fields_for :resultadopf, f.object.resultadopf.order(:id) do |o| %>
            <%= render 'resultadopf_campos', f:  o %>
          <% end %>
        </tbody>
      </table>
      <div class="links">
        <%= link_to_add_association t('Añadir resultado'), f, :resultadopf, {
          :"data-association-insertion-node" => "tbody#resultadospf", 
          :"data-association-insertion-method" => "append", 
          partial: 'resultadopf_campos',
          class: 'btn btn-sm btn-primary', 
          "data-ajax" => cor1440_gen.new_resultadospf_path,
          "data-ajaxdata" => "proyectofinanciero_id" } %>
      </div> <!-- link -->
    </div> <!-- .div-resultadospf -->
  </div> <!-- panel-body -->
</div> <!-- panel -->
